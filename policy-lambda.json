{
    "Id": "PolicyForLambdaExecutionRole",
    "Statement": [
        {
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Effect": "Allow",
            "Resource": "arn:aws:s3:::${bucket}/*"
        }
    ],
    "Version": "2012-10-17"
}
